{
  "functions": {
    "api/generate-puzzle.js": {
      "maxDuration": 30
    },
    "api/analyzeIncorrectMove.js": {
      "maxDuration": 15
    },
    "api/analyzeIncorrectMoveStockfish.js": {
      "maxDuration": 15
    },
    "api/analyzeMoveConsequences.js": {
      "maxDuration": 20
    },
    "api/analyzeMoveConsequencesStockfish.js": {
      "maxDuration": 25
    },
    "api/analyzeIncorrectMoveStockfishEnhanced.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "credentialless"
        },
        {
          "key": "Cross-Origin-Opener-Policy", 
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Resource-Policy",
          "value": "cross-origin"
        }
      ]
    }
  ]
}
